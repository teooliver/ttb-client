{
  "name": "ttb-client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:e2e": "playwright test",
    "test:snapshot-update": "playwright test --update-snapshots",
    "clean:test:ports": "npx kill-port 3000",
    "list:tests": "find . -type f -name '*.test.*' ! -path './node_modules/*'",
    "list:e2e-tests": "find . -type f -name '*.e2e.test.*' ! -path './node_modules/*'",
    "list:stories": "find . -type f -name '*.stories.*' ! -path './node_modules/*'"
  },
  "dependencies": {
    "@reach/dialog": "^0.17.0",
    "@reach/listbox": "^0.17.0",
    "@reach/menu-button": "^0.17.0",
    "chart.js": "^3.7.1",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-chartjs-2": "^4.1.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "18.1.0",
    "react-query": "^3.39.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@playwright/test": "^1.22.1",
    "@storybook/addon-actions": "^6.5.0",
    "@storybook/addon-essentials": "^6.5.0",
    "@storybook/addon-interactions": "^6.5.0",
    "@storybook/addon-links": "^6.5.0",
    "@storybook/builder-webpack5": "^6.5.0",
    "@storybook/manager-webpack5": "^6.5.0",
    "@storybook/react": "^6.5.0",
    "@storybook/testing-library": "^0.0.11",
    "@types/node": "^17.0.34",
    "@types/react": "18.0.9",
    "@types/react-datepicker": "^4.4.1",
    "babel-loader": "^8.2.5",
    "eslint-config-next": "12.1.6",
    "husky": "^8.0.1",
    "msw": "^0.40.0",
    "prettier": "^2.6.2",
    "typescript": "4.6.4"
  },
  "resolutions": {
    "webpack": "^5"
  }
}
